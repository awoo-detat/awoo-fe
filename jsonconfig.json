{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["assets/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@components/*": ["src/components/*"],
      "@navigation/*": ["src/navigation/*"],
      "@constants/*": ["src/constants/*"],
      "@screens/*": ["src/screens/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@analytics/*": ["src/analytics/*"],
      "@hooks/*": ["src/hooks/*"],
      "@mocks/*": ["src/__mocks__"]
    }
  },
  "exclude": ["**/__mocks__/*"]
}